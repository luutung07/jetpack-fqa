<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <corners android:radius="@dimen/fbase_dimen_16" />
            <solid android:color="@color/white" />
            <size android:width="@dimen/fbase_dimen_24" android:height="@dimen/fbase_dimen_24" />
            <stroke android:width="@dimen/fbase_dimen_2" android:color="@color/neutral_7" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <corners android:radius="@dimen/fbase_dimen_16" />
            <solid android:color="@color/white" />
            <size android:width="@dimen/fbase_dimen_24" android:height="@dimen/fbase_dimen_24" />
            <stroke android:width="@dimen/fbase_dimen_2" android:color="@color/primary" />
        </shape>
    </item>
</selector>
