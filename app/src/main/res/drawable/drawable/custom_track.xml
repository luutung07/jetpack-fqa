<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/neutral_7" />
            <corners android:radius="@dimen/fbase_dimen_20" />
            <size android:width="@dimen/fbase_dimen_24" android:height="@dimen/fbase_dimen_26" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/primary" />
            <corners android:radius="@dimen/fbase_dimen_20" />
            <size android:width="@dimen/fbase_dimen_24" android:height="@dimen/fbase_dimen_24" />
        </shape>
    </item>
</selector>
